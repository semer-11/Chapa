{
  "name": "semernur/chapa2",
  "description": "Laravel Package for Chapa API",
  "keywords": [
    "semernur",
    "chapa",
    "payment",
    "gateway"
  ],
  "homepage": "https://github.com/semer-11/chapa",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "SEMEREDIN NURGEBA",
      "email": "semernur11@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "guzzlehttp/guzzle": "^7.4.5",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Semernur\\Chapa\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Semernur\\Chapa\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Semernur\\Chapa\\ChapaServiceProvider"
      ],
      "aliases": {
        "Chapa": "Semernur\\Chapa\\ChapaFacade"
      }
    }
  },
  "minimum-stability": "dev"
}
